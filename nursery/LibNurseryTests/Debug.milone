module rec LibNurseryTests.Debug

// Provides functions to dump value to string for debugging and testing.

let private debugStringOpt (opt: string option) =
  match opt with
  | Some value -> "Some(" + value + ")"
  | None -> "None"

let opt (debug: 'T -> string) (opt: 'T option) : string = Option.map debug opt |> debugStringOpt
