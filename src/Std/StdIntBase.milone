module rec Std.StdIntBase

let tryParseInt32 (s: string) : int option =
  __nativeStmt (
    """
      bool str_to_int_checked(struct String, int32_t *);

      int32_t value;
      bool ok = str_to_int_checked({0}, &value);
    """,
    s
  )

  if __nativeExpr "ok" then
    let value: int = __nativeExpr "value"
    Some value
  else
    None

